      <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="owl-carousel owl-theme">
                    <% for (var i=0; i<certificate_details.length; i++) { %>
                    <div class="item">
                        <div class="certificate-card" data-toggle="modal" data-target="#certificateModal" 
                             data-title="<%=certificate_details[i].certificate_title%>" 
                             data-description="<%=certificate_details[i].certificate_desc%>" 
                             data-image="/uploads/<%=certificate_details[i].certificate_image%>" 
                             data-cerdate="<%=certificate_details[i].certificate_date%>">
                            <img src="/uploads/<%=certificate_details[i].certificate_image%>" alt="Certificate Image">
                            <h3><%=certificate_details[i].certificate_title%></h3>
                            <h3><%=certificate_details[i].certificate_date%></h3>
                            <h6><%=certificate_details[i].certificate_desc%></h6>
                        </div>
                    </div>
                    <% } %>
                </div>
            </div>
        </div>
    </div>

    <!-- Modal -->
    <% for (var i=0; i<certificate_details.length; i++) { %>
    <div class="modal fade" id="certificateModal" tabindex="-1" aria-labelledby="certificateModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="certificateModalLabel"></h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <img id="certificateImage" src="" alt="Certificate Image" class="img-fluid mb-3" width="100%">
                    <h3 id="certificateDescription" style="color: yellow; text-align: center;"></h3>
                    <h3 id="certificateDate" style="color: yellow; text-align: center;"></h3>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>
    <% } %>

    <!-- jQuery -->
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <!-- Bootstrap JS -->
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.bundle.min.js"></script>
    <!-- Owl Carousel JS -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js"></script>
    <!-- Initialize Owl Carousel -->
    <script>
        $(document).ready(function() {
            $(".owl-carousel").owlCarousel({
                items: 4,
                responsive: {
                    0: { items: 1 },    // 0-659px
                    660: { items: 1 },  // 660-979px
                    980: { items: 2 },  // 980-1198px
                    1199: { items: 4 }  // 1199px and above
                },
                nav: true,
                // dots: true,
                autoplay: true,
                autoplayTimeout: 3000, // Duration between slides in milliseconds
                autoplayHoverPause: true, // Pause on mouse hover
                loop: true,
                margin: 10
            });

            // Handle modal show event
            $('#certificateModal').on('show.bs.modal', function(event) {
                var button = $(event.relatedTarget); // Button that triggered the modal
                var title = button.data('title');
                var description = button.data('description');
                var image = button.data('image');
                var cerdate = button.data('cerdate');

                var modal = $(this);
                modal.find('.modal-title').text(title);
                modal.find('#certificateImage').attr('src', image);
                modal.find('#certificateDescription').text(description);
                modal.find('#certificateDate').text(cerdate);
            });
        });
    </script>
  </section>
