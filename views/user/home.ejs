  <!-- PROJECTS  -->

  <section id="projects" class="mt-5">
    <h2 class="text-center" style="font-size: 100px;">Projects</h2>
    <div class="container">
      <div class="row">
        <% for (let i = 0; i < project_info.length; i++) { %>
          <div class="col-lg-4 col-sm-12 col-md-6 mt-3  ">
            <div class="project_box" data-toggle="modal" data-target="#myModal-<%=i%>">
              <img src="/uploads/<%=project_info[i].project_image%>" alt="Project Image">
              <div class="overlay-text"><%=project_info[i].project_name%></div>
            </div>
          </div>
        <% } %>
      </div>
    </div>
    
    <!-- Modals -->
    <% for (let i = 0; i < project_info.length; i++) { %>
      <div class="modal fade" id="myModal-<%=i%>" tabindex="-1" role="dialog" aria-labelledby="myModalLabel-<%=i%>" aria-hidden="true">
        <div class="modal-dialog" role="document">
          <div class="modal-content">
            <div class="modal-header">
              <h5 class="modal-title" id="myModalLabel-<%=i%>"><%=project_info[i].project_name%></h5>
              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
              </button>
            </div>
            <div class="modal-body text-white">
              <p class="model_p_size"><b>Project Name</b> : <span class="model_size"><%=project_info[i].project_name%></span></p>
              <p class="model_p_size"><b>Project Role</b> : <span class="model_size"><%=project_info[i].project_role%></span></p>
              <p class="model_p_size"><b>Client</b> : <span class="model_size"><%=project_info[i].client_name%></span></p>
              <p class="model_p_size"><b>Project Technology</b> : <span class="model_size"><%=project_info[i].project_technology%></span></p>
              <p><b>Project Duration Starting Date To Project Duration End Date</b> : <span class="model_size"><%=project_info[i].project_duration%></span></p>
              <p class="model_p_size"><b>Project Description</b> : <span class="model_size"><%=project_info[i].project_desc%></span></p>
            </div>
            <div class="modal-footer">
              <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
            </div>
          </div>
        </div>
      </div>
    <% } %>
  </section>
  

